{
    "parser": "babel-eslint",
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings",
        "airbnb"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "new-cap": ["error", {"capIsNewExceptions": ["UglifyJS.Compressor"]}]
    },
    "env": {
        "browser": true
    }
}
