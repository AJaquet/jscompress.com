{
  "name": "jscompress.com",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "compile-uglifyjs": "uglifyjs --self -c -m -o ./node_modules/uglify-js/uglify-js-browser.js",
    "postinstall": "npm run compile-uglifyjs",
    "bundle": "NODE_ENV=production webpack --config ./tools/webpack.config.babel.js",
    "dev-server": "NODE_ENV=development webpack-dev-server --config tools/webpack.config.babel.js --port 8100",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "lint": "eslint . --ext .js --cache --ignore-pattern '**/node_modules/**' --ignore-pattern '**/dist/**'",
    "deploy": "pm2 deploy ecosystem.json production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/circlecell/jscompress.com.git"
  },
  "keywords": [
    "jscompress",
    "uglify"
  ],
  "author": "Circlecell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/circlecell/jscompress.com/issues"
  },
  "homepage": "https://github.com/circlecell/jscompress.com#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "npm-check-updates": "^2.7.2",
    "open-browser-webpack-plugin": "0.0.2",
    "scp2": "^0.5.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "exports-loader": "^0.6.3",
    "lodash.round": "^4.0.2",
    "matreshka": "^1.9.1",
    "uglify-js": "^2.7.0"
  },
  "peerDependencies": {
    "pm2": "^1.1.3"
  }
}
